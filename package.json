{
	"name": "graphql-pluck-types",
	"version": "0.0.0-development",
	"description": "Extract TypeScript interface definitions from a GraphQL schema SDL string",
	"license": "MIT",
	"repository": "mstuart/graphql-pluck-types",
	"author": {
		"name": "Mark Stuart",
		"email": "mstuart@users.noreply.github.com",
		"url": "https://github.com/mstuart"
	},
	"type": "module",
